package view

templ Layout(title string) {
	<html lang="en">
		<head>
			<title>{ title }</title>
			<meta charset="UTF-8"/>
			<meta name="viewport" content="width=device-width, initial-scale=1"/>
			<link href="/static/styles/main.css" rel="stylesheet"/>
			<script type="module" src="/static/app/main.js"></script>
		</head>
		<body>
			{ children... }
		</body>
	</html>
}
